eprint = {arXiv:1403.2094v2},
file = {:Users/lionel/Documents/2 - pro/00 - ressources/00 - Mendeley/2016 - “Wunderlich, meet kirchhoff” A general and unified description of elastic ribbons and thin rods - Audoly.pdf:pdf},
isbn = {9789401773003},
issn = {03743535},
journal = {The Mechanics of Ribbons and M{\"{o}}bius Bands},
keywords = {elastic plates 74k20,elastic rods 74k10,energy minimization},
pages = {49--66},
title = {{“Wunderlich, meet kirchhoff”: A general and unified description of elastic ribbons and thin rods}},
year = {2016}
}
