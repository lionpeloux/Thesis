@techreport{Lefevre2014,
abstract = {En utilisant la théorie des poutres de Kirchhoff, nous construisons un modèle de poutre élan- cée à section quelconque adapté aux calculs dynamiques et aux calculs statiques par relaxation dynamique. Ce modèle prend en compte la flexion et la torsion, et fait appel à une description cinématique réduite à 4 degrés de liberté, s'appuyant sur le repère de Bishop. Pour construire ce modèle, les efforts intérieurs sont calculés en dérivant l'énergie élastique sous forme intégrale. On obtient ainsi les efforts intérieurs linéiques pour une poutre conti- nue quelconque, laissant libre la discrétisation. Le modèle est validé quantitativement sur des exemples couplant flexion et torsion avec une discrétisation par interpolation linéaire. Nous proposons également un modèle de liaison entre deux poutres adapté à notre méthode de résolution, permettant ainsi de modéliser des structures complexes comme les gridshells. Ce travail permet donc la modélisation exacte des gridshells par relaxation dynamique. D'autres applications visant à l'étude dynamique ou statique des poutres élancées en inter- actions complexes peuvent être envisagées. Le formalisme continu proposé peut en outre servir des fins théoriques.},
address = {Paris},
author = {Lefevre, Baptiste},
file = {:Users/lionel/Documents/2 - pro/00 - ressources/00 - Mendeley/2014 - Modèle de poutre élancée à section quelconque en flexiontorsion - Lefevre.pdf:pdf},
institution = {Navier Ecole des Ponts ParisTech},
keywords = {bending,dynamic relaxation,gridshell,large rotations,rod,torsion},
pages = {53},
title = {{Modèle de poutre élancée à section quelconque en flexion/torsion}},
year = {2014}
}
@book{Monge1809,
annote = {Du rayon de courbure et des differens genres d"inflexions de courbes {\`{a}} double courbure.

