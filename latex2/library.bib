keywords = {Angular velocity,Pythagorean-hodograph curves,Quaternions,Rigid body motion,Rotation-minimizing frames,Ruled surfaces},
number = {1},
pages = {27--42},
publisher = {Elsevier B.V.},
title = {{Rotation-minimizing osculating frames}},
