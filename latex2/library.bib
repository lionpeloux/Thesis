pages = {227--235},
title = {{Timber gridshells: Numerical simulation, design and construction of a full scale structure}},
url = {http://www.sciencedirect.com/science/article/pii/S2352012415000557{\%}5Cnhttp://ac.els-cdn.com/S2352012415000557/1-s2.0-S2352012415000557-main.pdf?{\_}tid=46a49f84-9759-11e5-987a-00000aacb360{\&}acdnat=1448884719{\_}83b87dc36f196f9bd121fc9e4894e8f9},
